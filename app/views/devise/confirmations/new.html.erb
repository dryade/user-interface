<%= title_tag t('.title') %>

<%= semantic_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |form| %>
  <%= devise_error_messages! %>

  <%= form.input :email %> 

   <%= form.actions do %>
     <%= form.action :submit, :as => :button, :label => t('.title') %>
     <%= form.action :cancel, :as => :link %>
  <% end %>
<% end %>

<%= render "links" %>
